@model List<ASPNETMVCCRUD.Models.Domain.Employee>
@{
}

<h1>Employees</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>

                <td><a href="Employee/View/@employee.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
